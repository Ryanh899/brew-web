    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.0.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.20.1",
    "lodash": "^4.17.15",
    "material-table": "^1.53.0",
    "node-geocoder": "^3.25.0",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
